{
    "name": "renoki-co/laravel-exporter-contracts",
    "description": "Base contracts implementation for Prometheus exports in Laravel.",
    "keywords": ["laravel", "php", "prometheus", "metrics", "exporter"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-exporter-contracts",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "promphp/prometheus_client_php": "^2.3",
        "illuminate/config": "^8.40",
        "illuminate/routing": "^8.40",
        "illuminate/support": "^8.40"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LaravelExporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LaravelExporter\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.22.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\LaravelExporter\\LaravelExporterServiceProvider"
            ]
        }
    }
}
